CREATE TABLE ALUNO
    (prontuario VARCHAR (15),
     nome VARCHAR (50),
     idade INTEGER,
    CONSTRAINT PK_ALUNO PRIMARY KEY (prontuario));
    
SELECT nome,idade FROM ALUNO;

INSERT INTO ALUNO VALUES ('12AA', 'Mariana',15);
INSERT INTO ALUNO VALUES ('12BB', 'Victor',15);

--CRIANDO TABELA CURSO
CREATE TABLE CURSO
    (idCurso INTEGER,
    nomeCurso VARCHAR (15) NOT NULL,
     CONSTRAINT PK_CURSO PRIMARY KEY (idCurso));
     
-- DROP TABLE CURSO;
     
SELECT * FROM CURSO;

INSERT INTO CURSO VALUES ('1', 'INFO');
